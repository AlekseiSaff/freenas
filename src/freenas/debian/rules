#!/usr/bin/make -f
#export DH_VERBOSE = 1

%:
	dh $@

override_dh_auto_install:
	sh -c "\
		mkdir -p debian/truenas-files/etc; \
		cp -a root usr debian/truenas-files/; \
		cp etc/find* debian/truenas-files/etc/; \
		cp etc/iso_3166_2_countries.csv debian/truenas-files/etc/; \
		# They come from snmp-mibs-downloader post-install script \
		mkdir -p debian/var/lib/snmp; \
		cp -R /var/lib/snmp/mibs debian/var/lib/snmp; \
	"

override_dh_shlibdeps:

override_dh_usrlocal:
